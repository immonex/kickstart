{
  "name": "@immonex/kickstart",
  "version": "1.9.0",
  "description": "Essential components and add-on framework for embedding and searching/filtering imported OpenImmo-XML-based real estate offers",
  "private": true,
  "config": {
    "pluginFolderName": "immonex-kickstart"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.5",
    "axios": "^1.4.0",
    "debounce": "^1.2.1",
    "flatpickr": "^4.6.13",
    "nouislider": "^15.7.0",
    "ol": "^7.3.0",
    "qs": "^6.11.1",
    "uikit": "^3.19",
    "vue": "^2",
    "vue-cookies": "^1.8.3",
    "vue-multiselect": "^2.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "babel-loader": "^9.1.2",
    "browser-sync": "^2.29.1",
    "browser-sync-webpack-plugin": "^2.3.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "dotenv": "^16.0.3",
    "file-loader": "^6.2.0",
    "glob": "^10.2.3",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lws": "^4.1.2",
    "lws-index": "^3.1.1",
    "lws-static": "^3.1.0",
    "mini-css-extract-plugin": "^2.7.5",
    "node-sass": "^8.0.0",
    "rimraf": "^5.0.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "trim-newlines": ">=5.0.0",
    "vue-loader": "^15.1.0",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.8.0"
  },
  "scripts": {
    "clean": "rimraf --glob 'src/assets/@(js|css)' && rimraf --glob 'src/skins/**/assets/*' && rimraf --glob build/* && rimraf --glob dist/*",
    "prebuild": "composer install --no-dev && npm run clean -s",
    "build:scripts": "webpack --config webpack.prod.js",
    "build:scripts-dev": "webpack --config webpack.dev.js",
    "build:copy": "mkdir -p build && cp -LR src/* build",
    "build:clean-core": "rimraf --glob 'build/vendor/immonex/wp-free-plugin-core/!(src|assets|languages|README*|LICENSE)'",
    "build:clean-cmb2": "rimraf --glob 'build/vendor/cmb2/cmb2/languages/cmb2-!(de_*|en_*|fr_*|nl_*|es_*|it_*)' && rimraf --glob 'build/vendor/cmb2/cmb2/@(tests|apigen|coverage*)'",
    "build:clean-vendor": "rimraf --glob 'build/vendor/**/@(bin|sass|example*|Grunt*|grunt*|gulp*|phpunit*|Dock*|composer.*|package.json|package-lock.json)' && rimraf --glob 'build/vendor/**/.[^.]*'",
    "build:del-scss-js-sources": "rimraf --glob build/**/scss && rimraf --glob build/**/*.scss && rimraf --glob build/js && rimraf --glob build/skins/**/js",
    "build:zip": "mv build $npm_package_config_pluginFolderName && mkdir -p dist && zip -rq dist/$npm_package_config_pluginFolderName.zip $npm_package_config_pluginFolderName -x '**/.DS_Store' && mv $npm_package_config_pluginFolderName build",
    "build": "npm run build:scripts && npm run build:copy && npm run build:clean-core && npm run build:clean-cmb2 && npm run build:clean-vendor && npm run build:del-scss-js-sources && npm run build:zip",
    "postbuild": "composer install --ignore-platform-reqs",
    "watch": "webpack --watch --config webpack.dev.js",
    "pot:copy": "mkdir -p $npm_package_config_pluginFolderName && cp -R src/* $npm_package_config_pluginFolderName",
    "pot:extract": "php vendor/inverisoss/wp-i18n-tools/makepot.php wp-plugin $npm_package_config_pluginFolderName/ src/languages/$npm_package_config_pluginFolderName.pot vendor,assets,bin,data",
    "pot:delete": "rimraf --glob $npm_package_config_pluginFolderName",
    "pot": "npm run pot:copy && npm run pot:extract && npm run pot:delete",
    "test:install": "bin/invoke_test_installer.sh",
    "test": "vendor/phpunit/phpunit/phpunit -c tests/phpunit/plugin.xml",
    "doc": "doc-gen serve",
    "doc:publish": "doc-gen publish",
    "apidoc": "bin/phpdoc run --sourcecode",
    "apidoc:view": "node_modules/.bin/lws --stack lws-static lws-index --hostname localhost --port 8001 -d apidoc -o",
    "apidoc:delete": "rimraf --glob apidoc && rimraf --glob .tmp/apidoc"
  }
}