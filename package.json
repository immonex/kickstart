{
  "name": "@immonex/kickstart",
  "version": "1.0.0-rc1",
  "description": "Essential components and framework for embedding and searching/filtering imported OpenImmoÂ® based real estate offers",
  "private": true,
  "config": {
    "pluginFolderName": "immonex-kickstart"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "axios": "^0.18.1",
    "debounce": "^1.2.0",
    "nouislider": "^11.1.0",
    "ol": "^5.3.3",
    "qs": "^6.9.1",
    "uikit": "^3.3.6",
    "vue": "^2.6.11",
    "vue-cookies": "^1.7.0",
    "vue-multiselect": "^2.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "file-loader": "^5.1.0",
    "glob": "^7.1.6",
    "lws": "^3.0.0",
    "lws-index": "^2.0.0",
    "lws-static": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "vue-loader": "^15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-command": "^0.5.0",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "clean": "rimraf build/* && rimraf dist/*",
    "prebuild": "composer install --no-dev && npm run clean -s",
    "build:scripts": "webpack --config webpack.prod.js",
    "build:copy": "mkdir -p build && cp -LR src/* build",
    "build:cleanup-core": "rimraf 'build/vendor/immonex/wp-free-plugin-core/!(src|assets|languages|README*|LICENSE)'",
    "build:delete-vendor-dotfiles": "rimraf 'build/vendor/**/.[^.]*'",
    "build:delete-cmb2-lang-files": "rimraf 'build/vendor/cmb2/cmb2/languages/cmb2-!(de_*|en_*|fr_*|nl_*|es_*|it_*)'",
    "build:cleanup-cmb2": "rimraf 'build/vendor/cmb2/cmb2/@(tests|apigen|coverage*)'",
    "build:zip": "mv build $npm_package_config_pluginFolderName && mkdir -p dist && zip -rq dist/$npm_package_config_pluginFolderName.zip $npm_package_config_pluginFolderName && mv $npm_package_config_pluginFolderName build",
    "build": "npm run build:scripts -s && npm run build:copy -s && npm run build:cleanup-core -s && npm run build:delete-cmb2-lang-files -s && npm run build:cleanup-cmb2 -s && npm run build:delete-vendor-dotfiles -s && npm run build:zip -s",
    "postbuild": "composer install",
    "watch": "webpack --watch --config webpack.dev.js",
    "pot:copy": "mkdir -p $npm_package_config_pluginFolderName && cp -R src/* $npm_package_config_pluginFolderName",
    "pot:extract": "php vendor/inverisoss/wp-i18n-tools/makepot.php wp-plugin $npm_package_config_pluginFolderName/ src/languages/$npm_package_config_pluginFolderName.pot vendor,assets,bin,data",
    "pot:delete": "rimraf $npm_package_config_pluginFolderName",
    "pot": "npm run pot:copy && npm run pot:extract && npm run pot:delete",
    "test:install": "bin/invoke_test_installer.sh",
    "test": "vendor/phpunit/phpunit/phpunit -c tests/phpunit/plugin.xml",
    "doc": "doc-gen server",
    "doc:publish": "bin/publish_doc.sh",
    "apidoc": "vendor/bin/phpdoc run --sourcecode --template=clean --title='immonex Kickstart'",
    "apidoc:view": "node_modules/.bin/lws --stack lws-static lws-index -d apidoc -o",
    "apidoc:delete": "rimraf apidoc && rimraf .tmp/apidoc"
  }
}
